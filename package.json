{
  "name": "@niftyleague/smart-contracts",
  "version": "1.0.0",
  "description": "Nifty League Smart Contracts",
  "author": "Nifty Andy",
  "license": "MIT",
  "main": "index.js",
  "homepage": "https://github.com/NiftyLeague/NiftySmartContracts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiftyLeague/NiftySmartContracts"
  },
  "bugs": {
    "url": "https://github.com/NiftyLeague/NiftySmartContracts/issues"
  },
  "scripts": {
    "lint:sol": "solhint \"contracts/**/*.sol\"",
    "lint": "eslint --ignore-path .eslintignore . --ext .ts,.js",
    "format": "prettier --ignore-path .prettierignore --write .",
    "build": "npm run compile",
    "compile": "npx hardhat compile",
    "coverage": "npx hardhat coverage --network hardhat",
    "deploy:hardhat": "hardhat deploy --deploy-scripts ./deploy/hardhat",
    "deploy:remote": "hardhat deploy --deploy-scripts ./deploy/remote --network",
    "deploy:ledger": "hardhat deploy --deploy-scripts ./deploy/ledger --network",
    "test": "npx hardhat test"
  },
  "devDependencies": {
    "@ethersproject/bignumber": "^5.4.2",
    "@ethersproject/contracts": "^5.4.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.2",
    "@openzeppelin/contracts-upgradeable": "^4.3.2",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@tenderly/hardhat-tenderly": "^1.0.12",
    "@typechain/ethers-v5": "^7.1.2",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.3",
    "@types/ramda": "^0.27.45",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eth-provider": "^0.9.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.7",
    "fs": "0.0.1-security",
    "hardhat": "^2.6.5",
    "hardhat-gas-reporter": "^1.0.9",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "ramda": "^0.27.1",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/wallet": "^5.7.0",
    "@imtbl/imlogging": "^1.0.37",
    "@imtbl/imx-contracts": "^1.0.1",
    "@imtbl/imx-sdk": "^1.39.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-deploy": "^0.11.22",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "ts-command-line-args": "^2.3.1",
    "ts-generator": "^0.1.1",
    "tsconfig-paths": "^4.1.2"
  },
  "engines": {
    "node": ">=16.18.1",
    "npm": ">=8.0.0"
  }
}
